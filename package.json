{
  "name": "n7-course-api",
  "version": "1.0.0",
  "description": "The WEB API Rest for course.",
  "main": "src/server.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "start": "npm run build && node dist/server.js",
    "start:dev": "nodemon --exec babel-node src/server.js --inspect=3001",
    "build": "babel src --out-dir dist",
    "lint": "eslint src --ext .js",
    "lint:fix": "eslint src --fix --ext .js",
    "prettier": "prettier --check 'src/**/*.js'",
    "prettier:fix": "prettier --write 'src/**/*.js'",
    "prepare": "npm run lint:fix && npm run prettier:fix"
  },
  "dependencies": {
    "body-parser": "^1.20.1",
    "dotenv": "^8.2.0",
    "express": "^4.18.2",
    "helmet": "^3.22.0",
    "morgan": "^1.10.0",
    "mysql2": "^2.1.0",
    "sequelize": "^5.21.6"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/node": "^7.8.7",
    "@babel/preset-env": "^7.9.5",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-prettier": "^3.1.3",
    "nodemon": "^2.0.3",
    "prettier": "^2.0.4"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/danielcsrs/n7-course-api.git"
  },
  "keywords": [
    "nodejs",
    "express",
    "sequelize",
    "docker"
  ],
  "author": "@danielcsrs",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/danielcsrs/n7-course-api/issues"
  },
  "homepage": "https://github.com/danielcsrs/n7-course-api#readme"
}
